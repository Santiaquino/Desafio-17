<h1>Productos en este carrito:</h1>
<div class="containerProducts">
  {{#each products}}
  <div class="card" style="width: 18rem;">
    <img src="/img/{{this.title}}.png" class="card-img-top" alt="Imagen de la notebook de la marca {{this.title}}">
    <div class="card-body">
      <h5 class="card-title">{{this.title}}</h5>
      <p class="card-text"> <strong>description:</strong> {{description}} <br> <strong>code:</strong> {{code}}<br>
        <strong>price:</strong> {{price}}<br>
        <strong>status:</strong> {{status}}<br> <strong>stock:</strong> {{stock}}<br> <strong>category:</strong>
        {{category}}
      </p>
    </div>
  </div>
  {{/each}}
  <button class="btn btn-primary" id="buttonCompra">Comprar Carrito</button>
</div>

<script src="/js/carts.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
  integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
